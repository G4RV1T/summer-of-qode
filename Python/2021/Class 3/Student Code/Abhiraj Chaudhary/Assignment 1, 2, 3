#assignment 1
x = int(input("Enter a number: "))
while x>0 and x<50:
  print ("Number is between 0 and 50")
  y = int(input("Enter a number: "))
  if (y<0 or y>50):
    break
print ("Number is not between 0 and 50")

#assignment 2
x = 0
while (x<5):
  name = (input("Enter your name: "))
  print(name)
  x += 1
  
#assignment 3
x = 4
y = 1
while x<10:
  print(x*y)
  y += 1
  if (x*y==104):
    break
